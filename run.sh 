CUDA_VISIBLE_DEVICES=0 python main_train.py \
--resnet50 --runs_name resnet50_baseline \
--model resnet50

CUDA_VISIBLE_DEVICES=0 python main_train.py \
--resnet50 --runs_name graph-hnet-pseudo_baseline \
--model graph-hnet-pseudo

CUDA_VISIBLE_DEVICES=0 python main_train.py \
--resnet50 --runs_name graph-hnet-pseudo_k=7 \
--model graph-hnet-pseudo

CUDA_VISIBLE_DEVICES=0 python main_train.py \
--resnet50 --runs_name graph-hnet-pseudo_k=7_1024_layer1 \
--model graph-hnet-pseudo

python main_train.py \
--device 0 \
--resnet50 --runs_name graph-hnet-pseudo_k=5_1024_layer1_512 \
--model graph-hnet-pseudo

python main_train.py \
--device 0 \
--resnet50 --runs_name graph-hnet-pseudo_k=9 \
--model graph-hnet-pseudo

python main_train.py \
--device 0 \
--resnet50 --runs_name MRConv2d \
--model graph-hnet-pseudo